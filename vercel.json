{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/:path*",
      "has": [{"type": "host", "value": "www.pepperoni.tatar"}],
      "destination": "https://pepperoni.tatar/:path*",
      "permanent": true
    }
  ]
}
